ACTION=="add|change", KERNEL=="mmcblk[0-9]", KERNELS=="428b0000.mmc", SYMLINK+="lino-sd2"
ACTION=="add|change", KERNEL=="mmcblk[0-9]p[0-9]*", KERNELS=="428b0000.mmc", SYMLINK+="lino-sd2-part%n"
ACTION=="add|change", SUBSYSTEM=="spidev", KERNELS=="42720000.spi", SYMLINK+="lino-spi1-cs%n"
ACTION=="add|change", SUBSYSTEM=="spidev", KERNELS=="44360000.spi", SYMLINK+="lino-spi2-cs%n""
ACTION=="add|change", ATTRS{iomem_base}=="0x425A0010", SYMLINK+="lino-uart1"
ACTION=="add|change", ATTRS{iomem_base}=="0x42690010", SYMLINK+="lino-uart2"
ACTION=="add|change", ATTRS{iomem_base}=="0x44380010", SYMLINK+="lino-uart3
ACTION=="add|change", ATTRS{iomem_base}=="0x44390010", SYMLINK+="lino-uart4"
SUBSYSTEM=="iio", KERNELS=="iio:device0", RUN+="/etc/udev/scripts/toradex-adc.sh"
