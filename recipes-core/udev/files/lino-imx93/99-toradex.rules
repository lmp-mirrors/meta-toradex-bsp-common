KERNEL=="mmcblk[0-9]", ENV{DEVTYPE}=="disk", KERNELS=="42850000.mmc", SYMLINK+="emmc"
KERNEL=="mmcblk[0-9]boot[0-9]", ENV{DEVTYPE}=="disk", KERNELS=="42850000.mmc", SYMLINK+="emmc-boot%n"
KERNEL=="mmcblk[0-9]p[0-9]", ENV{DEVTYPE}=="partition", KERNELS=="42850000.mmc", SYMLINK+="emmc-part%n"
ACTION=="add|change", KERNEL=="i2c-[0-9]*", ATTR{name}=="44340000.i2c", SYMLINK+="lino-i2c1"
ACTION=="add|change", KERNEL=="i2c-[0-9]*", ATTR{name}=="42530000.i2c", SYMLINK+="lino-i2c2"
ACTION=="add|change", KERNEL=="i2c-[0-9]*", ATTR{name}=="42540000.i2c", SYMLINK+="lino-i2c3"
ACTION=="add|change", KERNELS=="watchdog", SYMLINK+="lino-watchdog"
ACTION=="add|change", SUBSYSTEM=="watchdog", KERNELS=="soc*", SYMLINK+="lino-watchdog-soc"
ACTION=="add|change", KERNEL=="mmcblk[0-9]", KERNELS=="42860000.mmc", SYMLINK+="lino-sd1"
